{
    "provider": {
        "name": "fournisseur"
    },
    "consumer": {
        "name": "client"
    },
    "interactions": [
        {
            "description": "Get artiste details for artiste id",
            "request": {
                "method": "GET",
                "path": "/artiste/2"
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json"
                },
                "body": {
                    "age": 21,
                    "name": "XXXTENTACION",
                    "noOfMusic": 1
                },
                "matchingRules": {
                    "body": {
                        "$.name": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.age": {
                            "matchers": [
                                {
                                    "match": "integer"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.noOfMusic": {
                            "matchers": [
                                {
                                    "match": "integer"
                                }
                            ],
                            "combine": "AND"
                        }
                    }
                }
            },
            "providerStates": [
                {
                    "name": "Get artiste details"
                }
            ]
        },
        {
            "description": "Get music details for music id",
            "request": {
                "method": "GET",
                "path": "/music/17"
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json"
                },
                "body": {
                    "artiste": "TONES AND I",
                    "name": "DANCE MONKEY",
                    "year": 2019
                },
                "matchingRules": {
                    "body": {
                        "$.artiste": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.name": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.year": {
                            "matchers": [
                                {
                                    "match": "integer"
                                }
                            ],
                            "combine": "AND"
                        }
                    }
                }
            },
            "providerStates": [
                {
                    "name": "Get music details"
                }
            ]
        }
    ],
    "metadata": {
        "pactSpecification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.5.24"
        }
    }
}